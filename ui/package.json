{
  "name": "@maas-ui/ui",
  "version": "0.2.0",
  "private": true,
  "dependencies": {
    "@canonical/macaroon-bakery": "0.3.0",
    "@canonical/react-components": "0.3.0",
    "@maas-ui/shared": "0.1.0",
    "classnames": "2.2.6",
    "connected-react-router": "6.6.1",
    "cross-env": "6.0.3",
    "date-fns": "2.8.1",
    "formik": "2.0.8",
    "http-proxy-middleware": "0.20.0",
    "immer": "5.0.2",
    "nanoid": "2.1.8",
    "path-parse": "1.0.6",
    "pluralize": "8.0.0",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-dropzone": "10.2.1",
    "react-redux": "7.1.3",
    "react-router-dom": "5.1.2",
    "react-router-hash-link": "1.2.2",
    "react-router-prop-types": "1.0.4",
    "react-scripts": "3.2.0",
    "react-useportal": "1.0.13",
    "reconnecting-websocket": "4.2.0",
    "reduce-reducers": "1.0.4",
    "redux": "4.0.4",
    "redux-devtools-extension": "2.13.8",
    "redux-saga": "1.1.3",
    "reselect": "4.0.0",
    "vanilla-framework": "2.5.0",
    "yup": "0.28.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "serve": "react-scripts start",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "release": "yarn clean && yarn install && CI=true yarn test && yarn build && yarn version --new-version",
    "postbuild": "mkdir -p dist && tar -C build -czvf dist/maas-ui-v$npm_package_version.tar.gz .",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "yarn lint-package-json && yarn lint-js",
    "lint-js": "eslint src",
    "lint-package-json": "npmPkgJsonLint .",
    "clean": "rm -rf node_modules build dist"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.15.1",
    "enzyme-to-json": "3.4.3",
    "eslint-plugin-no-only-tests": "2.4.0",
    "eslint-plugin-prettier": "3.1.2",
    "mock-socket": "9.0.2",
    "node-sass": "4.13.0",
    "npm-package-json-lint": "4.5.0",
    "prettier": "1.19.1",
    "redux-mock-store": "1.5.4",
    "redux-saga-test-plan": "4.0.0-rc.3"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "npmpackagejsonlint": {
    "rules": {
      "prefer-absolute-version-dependencies": "error",
      "prefer-absolute-version-devDependencies": "error",
      "prefer-alphabetical-dependencies": "error",
      "prefer-alphabetical-devDependencies": "error"
    }
  }
}
